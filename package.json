{
	"name": "dmojorganizer",
	"displayName": "DMOJOrganizer",
	"description": "A tool to manage submissions on DMOJ",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "dmojorganizer.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "dmojorganizer.fromTemplate",
				"title": "From Template"
			},
			{
				"command": "dmojorganizer.getSubs",
				"title": "Get Subs"
			},
			{
				"command": "dmojorganizer.Authenticate",
				"title": "Authenticate"
			},
			{
				"command": "dmojorganizer.Submit",
				"title": "Submit"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.44.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^10.2.0",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
